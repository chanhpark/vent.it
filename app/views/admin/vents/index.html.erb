<div class="row">
  <div class="small-10 large-centered columns">
    <h3> Admin Section </h3>
    <table>
      <tr>
        <td><strong>Vent Title</td>
          <td><strong>Content</td>
                </tr>
                <% @vents.each do |info| %>
                <tr>
                  <td><%= info.title %></td>
                  <td><%= info.content %></td>
                  <td><%= link_to 'Edit', edit_admin_vent_path(info) %></td>
                  <td><%= link_to "Delete", admin_vent_path(info), method: :delete, data: { confirm: "Are you sure?" } %></td>
                </tr>
                <% end %>
              </table>
            </div>
          </div>
